    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-relay": "^1.8.2",
    "eslint-plugin-testing-library": "^4.11.0",
    "graphql": "^15.5.1",
    "history": "^5.0.1",
    "prettier": "2.3.2",
    "react-router": "^5.1.2",
    "react-router-config": "^5.1.1",
    "relay-compiler": "^11.0.2",
    "relay-compiler-language-typescript": "^14.1.0",
    "relay-config": "^11.0.2"

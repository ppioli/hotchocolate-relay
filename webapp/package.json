{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "starty": "yarn run relay; concurrently --kill-others --names \"react-scripts,relay\" \"react-scripts start\" \"yarn run relay --watch\"",
